{
  "name": "ssa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx -n='-r dotenv/config' node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build-release-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && react-native run-android --variant=release",
    "build-release-bundle-ios": "react-native bundle --entry-file index.js --bundle-output ios/posted/main.jsbundle --dev=false --platform ios --assets-dest ios",
    "lint": "./node_modules/.bin/eslint ./src || true",
    "lintfix": "./node_modules/.bin/eslint ./src --fix || true"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "react": "16.6.3",
    "react-native": "^0.57.8",
    "react-native-camera": "^1.9.2",
    "react-native-qrcode-scanner": "^1.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "metro-react-native-babel-preset": "0.48.5"
  }
